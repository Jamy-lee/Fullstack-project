t type=\"number\" name=\"number\" v-model=\"number\" />\n\n    <label>Message:</label>\n    <input type=\"text\" name=\"Message\" v-model=\"message\" />\n    <br />\n    <button type=\"submit\" class=\"btn\">Submit</button>\n    <button type=\"reset\" class=\"btn\">Reset</button>\n    <!-- {{ name }}\n    {{ email }}\n    {{ number }}\n    {{ message }} -->\n  </form>\n  <div id=\"footer\">@ Jamy-lee Lewin</div>\n</template>\n\n<script>\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      number: \"\",\n      message: \"\",\n    };\n  },\n  methods: {\n    handleSubmit() {\n      console.log(this.name, this.email, this.contact, this.message);\n      fetch(\"https://vue-portfolio-3.herokuapp.com/contact\", {\n        method: \"POST\",\n        body: JSON.stringify({\n          name: this.name,\n          email: this.email,\n          number: this.number,\n          message: this.message,\n        }),\n        headers: {\n          \"Content-type\": \"application/json; charset=UTF-8\",\n        },\n      })\n        .then((response) => response.json())\n        .then((json) => {console.log(json)\n        alert(\"Message sent successfully\")\n        this.name = \"\",\n        this.email = \"\",\n        this.number = \"\",\n        this.message = \"\"\n        });\n    },\n  },\n};\n</script>\n\n<style>\nh3 {\n  margin-top: 10px;\n  font-family: monospace;\n  text-align: center;\n  color: #f08c09;\n  font-weight: bold;\n  font-size: 300%;\n}\n.btn {\n  color: white;\n  background-color: #f08c09;\n}\n#footer {\n  padding: 5px;\n  bottom: 0;\n  width: 100%;\n  background-color: #f08c09;\n  color: white;\n  text-align: right;\n  font-family: monospace;\n  font-size: 10px;\n  height: 30px;\n}\nform {\n  max-width: 400px;\n  margin: 30px auto;\n  margin-top: 80px;\n  background: white;\n  text-align: left;\n  padding: 40px;\n  border-radius: calc(var(--curve) * 1px);\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px